<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <mapped-superclass name="NicolasJoubert\GrabitBundle\Entity\Template" table="grabit_template">
        <id name="id" column="id" type="integer">
            <generator strategy="AUTO" />
        </id>
        <field name="code" column="code" type="string" length="25" />
        <field name="label" column="label" type="string" length="100" />
        <field name="configuration" column="configuration" type="json" nullable="true" />
    </mapped-superclass>
</doctrine-mapping>
