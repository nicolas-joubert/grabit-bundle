{
  "name": "nicolas-joubert/grabit-bundle",
  "description": "Grab it is R&D crawler to grab HTML pages",
  "keywords": ["grab", "crawler", "html", "extract", "proxy"],
  "homepage": "https://github.com/nicolas-joubert/grabit-bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Joubert",
      "homepage": "https://www.nvirenque.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "NicolasJoubert\\GrabitBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NicolasJoubert\\GrabitBundle\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/dbal": "^3||^4",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0||^4.0",
    "dunglas/doctrine-json-odm": "^1.4",
    "symfony/console": "7.2.*",
    "symfony/css-selector": "7.2.*",
    "symfony/dom-crawler": "7.2.*",
    "symfony/dotenv": "7.2.*",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "7.2.*",
    "symfony/http-client": "7.2.*",
    "symfony/runtime": "7.2.*",
    "symfony/validator": "7.2.*",
    "symfony/yaml": "7.2.*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "*",
    "phpstan/extension-installer": "*",
    "phpstan/phpstan": "*",
    "phpstan/phpstan-doctrine": "*",
    "phpstan/phpstan-symfony": "*",
    "rector/rector": "*"
  },
  "suggest": {
    "nicolas-joubert/grabit-front-feed-bundle": "Atom feed frontend for GrabitBundle.",
    "nicolas-joubert/grabit-admin-sonata-bundle": "Sonata Admin for GrabitBundle."
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "phpstan/extension-installer": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "sort-packages": true
  }
}
